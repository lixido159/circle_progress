<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressBar">
        <attr name="text_size" format="dimension"/><!--中间字体大小 -->
        <attr name="duration" format="integer"/><!--转到满所需要时间 -->
        <attr name="radius" format="dimension"/><!--圆半径 -->
        <attr name="scale" format="integer"/><!--圆与宽的比 -->
        <attr name="color_background" format="color"/><!--没有填充的颜色 -->
        <attr name="color_foreground" format="color"/><!--填充的颜色 -->
        <attr name="color_text" format="color"/><!--中间字体颜色 -->
        <attr name="text_inside" format="string"/><!--中间字内容 -->
    </declare-styleable>
</resources>